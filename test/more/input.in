extern number printf(...);
extern number scanf(...);

number C[101][101];

def number rd() {
  number tmp;
  scanf("%lf", &tmp);
  return tmp;
}

def number main() {
  number n, m;

  n = rd(); m = rd();

  printf("(n,m) = %f %f\n", n, m);

  for (number i = 0; i<=n; i=i+1)
    C[i][0] = 1;
  
  for (number i = 1; i<=n; i=i+1)
    for (number j = 1; j<=i; j=j+1)
      C[i][j] = C[i-1][j-1] + C[i-1][j];
  
  printf("%.0f", C[n][m]);

  return 0;
}
